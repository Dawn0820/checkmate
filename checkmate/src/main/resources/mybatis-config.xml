<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >

<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.checkmate.member.model.vo.Member" alias="member"/>
		<typeAlias type="com.kh.checkmate.board.sharingInformation.model.vo.SharingInformation" alias="SharingInformation"/>
		<typeAlias type="com.kh.checkmate.studyGroup.model.vo.StudyGroup" alias="studyGroup"/>
		<typeAlias type="com.kh.checkmate.chat.model.vo.Chat" alias="chat"/>
		<typeAlias type="com.kh.checkmate.message.model.vo.Message"	alias="message" />
	</typeAliases>
		 
	<mappers>
		<mapper resource="/mappers/member/member-mapper.xml"/> 
		<mapper resource="/mappers/sharingInformation-mapper.xml"/>
		<mapper resource="/mappers/studyGroup/studyGroup-mapper.xml"/>
		<mapper resource="/mappers/chat/chat-mapper.xml"/>
		<mapper resource="/mappers/message/message-mapper.xml" />
	</mappers>

</configuration>
